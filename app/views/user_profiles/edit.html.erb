

<%= form_for @user_profile do |f| %>

<h3><%= f.label :name%>:

 <%= f.text_field :name %><br /></h3>

  <h3><%= f.label :number %>:

 <%= f.text_field :number %><br /></h3>

  <h3><%= f.label :description %>:

 <%= f.text_area :description %><br /></h3>

  <div class="field">
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </div>


 <%= f.submit %>
<% end %>