<div class="jumbotron">
<div class="media">
  <div class="media-left">

  <div class="container-fluid">
<p>
  <strong>Image:</strong>
  <%= image_tag @user_profile.image %>
</p>


<% if %>
  <% @user_profile.id === current_user.id %>
  </div>
  <div class="media-body">
    <h1 class="media-heading">Welcome <%= @user_profile.name   %> !</h1>
  	<h2>Phone Number on recond: <%= @user_profile.number  %></h2>
  	<h5>About me: <%= @user_profile.description  %></h5>
  	<p><%= link_to "Edit Profile", edit_user_profile_path(@user_profile.id)  %></p>    
  </div>

<% else %>
  <div class="media-body">
    <h1 class="media-heading"><%= @user_profile.name   %>'s Profile</h1>
    <h2>Phone Number on recond: <%= @user_profile.number  %></h2>
    <h5>About <%=@user_profile.name  %>: <%= @user_profile.description  %></h5> !!!
  

  </div>
<% end %>
</div>
</div>
  

</div>